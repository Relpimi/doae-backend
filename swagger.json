{
    "openapi": "3.0.0",
    "info": {
        "title": "Doaê",
        "description": "Api de doação para instituições sociais",
        "version": "1.0.0"
    },
    "server": [
        {
            "url": "http://localhost:1000/"
        }
    ],
    "basePath": "/",
    "paths": {
        "/institution": {
            "post": {
                "sumary": "Cadastro da instituicao",
                "description": "Essa rota será responsável por cadastrar as instituições",
                "tags": [
                    "Instituicao"
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/institution"
                            },
                            "examples": {
                                "institution": {
                                    "value": {
                                        "name":  "Lar das moças cegas",
                                        "cnpj": "12123456000112",
                                        "email": "contato@lardasmocascegas.com",
                                        "password": "Lardasmocas@123",
                                        "description": "Nosso objetivo é ajudar moças com deficiência visual.",
                                        "agency": "0001",
                                        "count": "000225558",
                                        "pix": "12123456000112",
                                        "phone": "2732560000",
                                        "cep": "29190010",
                                        "site": "lardasmocascegas.com"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json":{
                                "schema": {
                                    "type": "object",
                                    "$ref": "#/components/schemas/institution"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT"
            }
        },
        "schemas": {
            "session-request": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    }
                }
            },
            "institution": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "id_external": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "verified": {
                        "type": "string"
                    },
                    "cnpj": {
                        "type": "string"
                    },
                    "logo": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "agency": {
                        "type": "string"
                    },
                    "count": {
                        "type": "string"
                    },
                    "pix": {
                        "type": "string"
                    },
                    "phone": {
                        "type": "string"
                    },
                    "cep": {
                        "type": "string"
                    },
                    "site": {
                        "type": "string"
                    },
                    "createdAt": {
                        "type": "string"
                    },
                    "updatedAt": {
                        "type": "string"
                    },
                    "suspend": {
                        "type": "boolean"
                    }
                }
            }
        }
    },
    "securitySchemes": {
        "bearerAuth": {
            "type": "http",
            "scheme": "bearer",
            "bearerFormat": "JWT"
        }
    }
}